{
  "edges": [
    {
      "from": "input-hacker-news",
      "to": "urlTemplate",
      "out": "input-hacker-news",
      "in": "id"
    },
    {
      "from": "urlTemplate",
      "to": "fetch-url",
      "out": "url",
      "in": "url"
    },
    {
      "from": "fetch-url",
      "to": "promptTemplate",
      "out": "response",
      "in": "content"
    },
    {
      "from": "promptTemplate",
      "to": "generateCompletion",
      "out": "prompt",
      "in": "text"
    },
    {
      "from": "secrets",
      "to": "generateCompletion",
      "out": "CLAUDE_API_KEY",
      "in": "CLAUDE_API_KEY"
    },
    {
      "from": "input-model",
      "to": "generateCompletion",
      "out": "model",
      "in": "model"
    },
    {
      "from": "generateCompletion",
      "to": "output",
      "out": "completion",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "generateCompletion",
      "type": "generateCompletion"
    },
    {
      "id": "secrets",
      "type": "secrets",
      "configuration": {
        "keys": [
          "CLAUDE_API_KEY"
        ]
      }
    },
    {
      "id": "input-hacker-news",
      "type": "input",
      "configuration": {
        "message": "Which Hacker News post would you like to generate a summary for?"
      }
    },
    {
      "id": "urlTemplate",
      "type": "urlTemplate",
      "configuration": {
        "template": "https://news.ycombinator.com/item?id={id}"
      }
    },
    {
      "id": "fetch-url",
      "type": "fetch",
      "configuration": {
        "raw": true
      }
    },
    {
      "id": "promptTemplate",
      "type": "promptTemplate",
      "configuration": {
        "template": "Summarize the following Hackernews comment section: {{content}}\n\\n## Summary:\n"
      }
    },
    {
      "id": "input-model",
      "type": "input",
      "configuration": {
        "message": "What is your model?"
      }
    },
    {
      "id": "output",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    },
    {
      "url": "npm:@paulkinlan/claude-breadboard-kit"
    }
  ]
}